{
  "name": "dorm",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "clean:dist": "rimraf dist",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix=true",
    "test": "ng test",
    "format:write": "prettier src/**/*.{ts,json,md,less} --write",
    "format:test": "prettier src/**/*.{ts,json,md,less} --list-different",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commit-message-validator",
      "pre-push": "npm run build && npm run clean:dist"
    }
  },
  "lint-staged": {
    "*.{ts,json,md,less}": "prettier --write",
    "*.{ts}": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular/cli": "~8.3.24",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "commit-message-validator": "^0.1.11",
    "husky": "^4.2.3",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "prettier": "1.19.1",
    "lint-staged": "^10.0.8",
    "typescript": "~3.5.3"
  },
  "config": {
    "commit-message-validator": {
      "pattern": "^DORM-(XXX|\\d{1,5}) (build|ci|chore|docs|feat|fix|perf|refactor|revert|style|test) ?\\((\\*|.{1,20})\\): ",
      "errorMessage": "Commit message does not satisfy the requirements."
    }
  }
}
